<h1>Listing documents</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Title</th>
    <th>Updated</th>
    <th>Type</th>
    
  </tr>

<% for document in @documents %>
  <tr>
    <td><%=h document[:id] %></td>
    <td><%=h document[:title] %></td>
    <td><%=h document[:updated] %></td>
    <td><%=h document[:type] %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New document', new_document_path %>
